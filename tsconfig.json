{
  "compilerOptions": {
    "target": "ESNext",
    "baseUrl": ".",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "resolveJsonModule": true,
    "types": [
      "vite-node/client",
      "vite/client"
    ],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}