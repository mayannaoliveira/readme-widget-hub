{
  "title": "Star History",
  "description": "Star History 一个可以显示 GitHub 项目的 star 历史图的徽章。",
  "description:en-US": "The missing star history graph of GitHub repos.",
  "tag": {
    "repo": "https://github.com/star-history/star-history",
    "onlineTool": "https://www.star-history.com",
    "officialDoc": "https://www.star-history.com/blog/how-to-use-github-star-history"
  },
  "linkRules": {
    "querys": [
      {
        "name": "repos",
        "type": "string",
        "required": true,
        "description": "仓库名",
        "description:en-US": "Repository name",
        "moreDescription": "多个仓库名用 `,` 分隔，例如：`owner1/repo1,owner2/repo2`。",
        "moreDescription:en-US": "Multiple repository names separated by `,`, such as: `owner1/repo1,owner2/repo2`."
      },
      {
        "name": "type",
        "type": "string",
        "description": "显示类型",
        "description:en-US": "Display type",
        "moreDescription": "可选值：`Date` `Timeline`。",
        "moreDescription:en-US": "Supported: `Date` `Timeline`."
      }
    ],
    "uriTemplate": "https://api.star-history.com/svg{?repos,type}"
  },
  "examples": {
    "annotate": "star-history/star-history",
    "alt": "Star History Chart",
    "href": "https://api.star-history.com/svg?repos=star-history/star-history&type=Date",
    "images": [
      {
        "alt": "Star History Chart",
        "href": "https://api.star-history.com/svg?repos=star-history/star-history&type=Date"
      }
    ]
  }
}