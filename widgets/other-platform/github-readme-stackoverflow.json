{
  "title": "Github Stackoverflow Readme",
  "title:en-US": "Github Stackoverflow Readme",
  "title:pt-BR": "Github Stackoverflow Readme",
  "description": "在您的 Readme 中展示您的 StackOverflow 个人资料状态！",
  "description:en-US": "Show your StackOverflow profile status on your READMEs!",
  "description:pt-BR": "Exiba o seu perfil do StackOverflow no seu README!",
  "tag": {
    "repo": "https://github.com/omidnikrah/github-readme-stackoverflow",
    "onlineTool": "https://stackoverflow.com/",
    "officialDoc": "https://github.com/omidnikrah/github-readme-stackoverflow"
  },
  "linkRules": {
    "querys": [
      {
        "name": "userId",
        "type": "number",
        "required": true,
        "description": "StackOverflow 用户 ID。",
        "description:en-US": "Username in StackOverflow.",
        "description:pt-BR": "Nome do usuário no StackOverflow."
      },
      {
        "name": "theme",
        "type": "string",
        "default": "light",
        "description": "主题色",
        "description:en-US": "Theme of the StackOverflow card",
        "description:pt-BR": "Tema do cartão StackOverflow",
        "moreDescription": "可选值：`light` 或 `dark`",
        "moreDescription:en-US": "Supported: `light` or `dark`.",
        "moreDescription:pt-BR": "Suporta: `light` or `dark`."
      },
      {
        "name": "layout",
        "type": "string",
        "default": "default",
        "description": "StackOverflow 卡片的布局",
        "description:en-US": "Username in StackOverflow",
        "description:pt-BR": "Nome do usuário no StackOverflow",
        "moreDescription": "可选值：`compact` 或 `default`",
        "moreDescription:en-US": "Supported: `compact` or `default`.",
        "moreDescription:pt-BR": "Suporta: `compact` or `default`."
      }
    ],
    "uriTemplate": "https://github-stackoverflow-readme.vercel.app/{?userId,theme,layout}"
  },
