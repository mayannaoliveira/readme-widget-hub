{
  "title": "Stackoverflow Readme Profile",
  "description": "轻松分享你的 Stackoverflow 个人资料到你的 README。",
  "description:en-US": "Easily share your Stackoverflow's profile on your README.",
  "tag": {
    "repo": "https://github.com/johannchopin/stackoverflow-readme-profile",
    "onlineTool": "https://stackoverflow-readme-profile.vercel.app/",
    "officialDoc": "https://github.com/johannchopin/stackoverflow-readme-profile?tab=readme-ov-file#stackoverflow-readme-profile"
  },
  "linkRules": [
    {
      "params": [
        {
          "name": "template",
          "type": "string",
          "description": "显示模板",
          "description:en-US": "Display template",
          "moreDescription": "可选值：`profile` `profile-small`。",
          "moreDescription:en-US": "Supported: `profile` `profile-small`."
        },
        {
          "name": "userId",
          "type": "string",
          "description": "Stackoverflow 用户 ID",
          "description:en-US": "Stackoverflow user ID"
        }
      ],
      "querys": [
        {
          "name": "theme",
          "type": "string",
          "default": "default",
          "description": "主题",
          "description:en-US": "Theme to use",
          "moreDescription": "可选值：`default` `dark` `cobalt` `monokai` `graywhite` `hotdog`。",
          "moreDescription:en-US": "Supported: `default` `dark` `cobalt` `monokai` `graywhite` `hotdog`."
        },
        {
          "name": "website",
          "type": "boolean",
          "default": "true",
          "description": "是否显示网站 URL",
          "description:en-US": "Show or not the website URL",
          "moreDescription": "仅当 `template=profile` 时可用，可选值：`true` `false`。",
          "moreDescription:en-US": "Only when `template=profile` is available, supported: `true` `false`."
        },
        {
          "name": "location",
          "type": "boolean",
          "default": "true",
          "description": "是否显示位置",
          "description:en-US": "Show or not the location",
          "moreDescription": "仅当 `template=profile` 时可用，可选值：`true` `false`。",
          "moreDescription:en-US": "Only when `template=profile` is available, supported: `true` `false`."
        }
      ],
      "description": "Stackoverflow 个人资料",
      "description:en-US": "Stackoverflow's profile",
      "uriTemplate": "https://stackoverflow-readme-profile.johannchopin.fr/{template}/{userId}{?theme,website,location}"
    },
    {
      "params": [
        {
          "name": "tagName",
          "type": "string",
          "description": "标签名称",
          "description:en-US": "Tag name"
        },
        {
          "name": "userId",
          "type": "string",
          "description": "Stackoverflow 用户 ID",
          "description:en-US": "Stackoverflow user ID"
        }
      ],
      "querys": [
        {
          "name": "theme",
          "type": "string",
          "default": "default",
          "description": "主题",
          "description:en-US": "Theme to use",
          "moreDescription": "可选值：`default` `dark` `cobalt` `monokai` `graywhite` `hotdog`。",
          "moreDescription:en-US": "Supported: `default` `dark` `cobalt` `monokai` `graywhite` `hotdog`."
        }
      ],
      "description": "标签排行榜",
      "description:en-US": "Tag league ranking",
      "uriTemplate": "https://stackoverflow-readme-profile.johannchopin.fr/tags-league-ranking/{tagName}/{userId}{?theme}"
    }
  ],
  "examples": {
    "annotate": "johannchopin",
    "alt": "Stackoverflow Profile",
    "href": "https://stackoverflow-readme-profile.johannchopin.fr/profile/8583669?theme=dark",
    "images": [
      {
        "alt": "Stackoverflow Profile",
        "href": "https://stackoverflow-readme-profile.johannchopin.fr/profile/8583669?theme=dark"
      }
    ]
  }
}