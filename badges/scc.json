{
  "title": "Sloc Cloc and Code",
  "description": "一个可以显示代码行数、注释行数等的徽章服务。",
  "description:en-US": "scc is a very fast accurate code counter with complexity calculations and COCOMO estimates written in pure Go.",
  "tag": {
    "repo": "https://github.com/boyter/scc/",
    "officialDoc": "https://github.com/boyter/scc/?tab=readme-ov-file#badges-beta"
  },
  "linkRules": {
    "params": [
      {
        "name": "provider",
        "type": "string",
        "description": "提供商",
        "description:en-US": "Provider",
        "moreDescription": "可选值：`github`。",
        "moreDescription:en-US": "Supported: `github`."
      },
      {
        "name": "user",
        "type": "string",
        "description": "用户名",
        "description:en-US": "Username"
      },
      {
        "name": "repo",
        "type": "string",
        "description": "仓库名",
        "description:en-US": "Repository name"
      }
    ],
    "querys": [
      {
        "name": "category",
        "type": "string",
        "default": "code",
        "description": "统计类别",
        "description:en-US": "Category",
        "moreDescription": "可选值：`code` `blanks` `lines` `comments` `cocomo`。",
        "moreDescription:en-US": "Supported: `code` `blanks` `lines` `comments` `cocomo`."
      },
      {
        "name": "avg-wage",
        "type": "number",
        "default": "56286",
        "description": "平均工资",
        "description:en-US": "Average wage",
        "moreDescription": "值必须为正整数，否则将恢复到默认值 `56286`。",
        "moreDescription:en-US": "Value must be a positive integer, otherwise it will be restored to the default value `56286`."
      },
      {
        "name": "lower",
        "type": "boolean",
        "description": "将标题文本转换为小写",
        "description:en-US": "Lowercase title text"
      },
      {
        "name": "font-color",
        "type": "string",
        "description": "字体颜色",
        "description:en-US": "Font color",
        "moreDescription": "支持类型：`HEX`，例如：`FF0000`。",
        "moreDescription:en-US": "Supported: `HEX`, e.g. `FF0000`."
      },
      {
        "name": "font-shadow-color",
        "type": "string",
        "description": "字体阴影颜色",
        "description:en-US": "Font shadow color",
        "moreDescription": "支持类型：`HEX`，例如：`FF0000`。",
        "moreDescription:en-US": "Supported: `HEX`, e.g. `FF0000`."
      },
      {
        "name": "top-shadow-accent-color",
        "type": "string",
        "description": "顶部阴影突出颜色",
        "description:en-US": "Top shadow accent color",
        "moreDescription": "支持类型：`HEX`，例如：`FF0000`。",
        "moreDescription:en-US": "Supported: `HEX`, e.g. `FF0000`."
      },
      {
        "name": "title-bg-color",
        "type": "string",
        "description": "标题背景颜色",
        "description:en-US": "Title background color",
        "moreDescription": "支持类型：`HEX`，例如：`FF0000`。",
        "moreDescription:en-US": "Supported: `HEX`, e.g. `FF0000`."
      },
      {
        "name": "badge-bg-color",
        "type": "string",
        "description": "徽章背景颜色",
        "description:en-US": "Badge background color",
        "moreDescription": "支持类型：`HEX`，例如：`FF0000`。",
        "moreDescription:en-US": "Supported: `HEX`, e.g. `FF0000`."
      }
    ],
    "uriTemplate": "https://sloc.xyz/{provider}/{user}/{repo}{?category,avg-wage,lower,font-color,font-shadow-color,top-shadow-accent-color,title-bg-color,badge-bg-color}"
  },
  "examples": {
    "annotate": "xiaohuohumax/readme-widget-hub",
    "alt": "总行数",
    "alt:en-US": "Total lines",
    "href": "https://sloc.xyz/github/xiaohuohumax/readme-widget-hub",
    "images": [
      {
        "alt": "总行数",
        "alt:en-US": "Total lines",
        "href": "https://sloc.xyz/github/xiaohuohumax/readme-widget-hub"
      }
    ]
  }
}