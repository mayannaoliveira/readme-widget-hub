{
  "title": "Profile views counter",
  "description": "GitHub 用户资料查看计数器：显示月度统计数据。",
  "description:en-US": "GitHub profile views tracker: displaying monthly view statistics.",
  "tag": {
    "repo": "https://github.com/u8views/go-u8views",
    "onlineTool": "https://u8views.com/",
    "needRegister": true
  },
  "linkRules": {
    "params": [
      {
        "name": "userId",
        "type": "string",
        "description": "用户 ID",
        "description:en-US": "User ID"
      },
      {
        "name": "type",
        "type": "string",
        "description": "统计类型",
        "description:en-US": "Statistics type",
        "moreDescription": "可选值：`day-week-month-total-count`（全部） `day-count`（日） `week-count`（周）`month`（月度）`total-count`（总计）。",
        "moreDescription:en-US": "Supported: `day-week-month-total-count` (all) `day-count` (day) `week-count` (week) `month` (monthly) `total-count` (total)."
      }
    ],
    "uriTemplate": "https://u8views.com/api/v1/github/profiles/{userId}/views/{type}.svg"
  },
