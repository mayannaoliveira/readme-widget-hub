{
  "title": "Git Animals",
  "description": "通过 GitHub 活动培养你的宠物！",
  "description:en-US": "Grow your pets through GitHub activities!",
  "tag": {
    "repo": "https://github.com/git-goods/gitanimals",
    "onlineTool": "https://www.gitanimals.org/en_US",
    "officialDoc": "https://github.com/git-goods/gitanimals?tab=readme-ov-file#%EB%B9%A0%EB%A5%B4%EA%B2%8C-%EC%82%AC%EC%9A%A9%ED%95%98%EA%B8%B0"
  },
  "linkRules": [
    {
      "params": [
        {
          "name": "username",
          "type": "string",
          "description": "Github 用户名",
          "description:en-US": "Github username"
        },
        {
          "name": "mode",
          "type": "string",
          "description": "模式",
          "description:en-US": "Mode",
          "moreDescription": "可选值：`lines`（行模式）`farms`（农场模式）。",
          "moreDescription:en-US": "Supported: `lines`(Line Mode) `farms`(Farm Mode)."
        }
      ],
      "querys": [
        {
          "name": "contribution-view",
          "type": "boolean",
          "default": "true",
          "description": "是否在动物头上显示贡献数量。",
          "description:en-US": "Whether to show the contribution count on the animal's head.",
          "moreDescription": "可选值：`true` `false`。",
          "moreDescription:en-US": "Supported: `true` `false`."
        }
      ],
      "description": "行/农场模式",
      "description:en-US": "Line/Farm Mode",
      "uriTemplate": "https://render.gitanimals.org/{mode}/{username}"
    },
    {
      "params": [
        {
          "name": "guildId",
          "type": "string",
          "description": "公会 ID",
          "description:en-US": "Guild ID",
          "moreDescription": "需要去官网[创建公会](https://www.gitanimals.org/en_US/guild/create)获得。",
          "moreDescription:en-US": "Please create a guild on the official website [here](https://www.gitanimals.org/en_US/guild/create)."
        }
      ],
      "description": "公会模式",
      "description:en-US": "Guild Mode",
      "uriTemplate": "https://render.gitanimals.org/guilds/{guildId}/draw"
    }
  ],
