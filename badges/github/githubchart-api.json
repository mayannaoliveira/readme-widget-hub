{
  "title": "Github Chart API",
  "description": "回顾 Github 个人贡献图表，展示你的 Github 贡献历史。",
  "description:en-US": "📅 Embed github contributions chart as image.",
  "tags": {
    "repoUrl": "https://github.com/2016rshah/githubchart-api",
    "officialDocsUrl": "https://github.com/2016rshah/githubchart-api"
  },
  "rules": [
    {
      "params": [
        {
          "name": "color",
          "type": "string",
          "default": "default",
          "description": "主题颜色",
          "description:en-US": "Use theme",
          "extDescription": "可选值：`teal` `halloween` `default` `old` `HEX`（例如：`FF0000`）。",
          "extDescription:en-US": "Supported:`teal` `halloween` `default` `old` `HEX` (e.g. `FF0000`)."
        },
        {
          "name": "username",
          "type": "string",
          "description": "Github 用户名",
          "description:en-US": "Github username"
        }
      ],
      "alt": "配置颜色",
      "alt:en-US": "Configure color",
      "srcTemplate": "https://ghchart.rshah.org/{color}/{username}"
    },
    {
      "params": [
        {
          "name": "username",
          "type": "string",
          "description": "Github 用户名",
          "description:en-US": "Github username"
        }
      ],
      "srcTemplate": "https://ghchart.rshah.org/{username}"
    }
  ],
  "examples": [
    {
      "explain": {
        "alt": "贡献图表",
        "alt:en-US": "Github chart",
        "src": "https://ghchart.rshah.org/xiaohuohumax"
      },
      "images": [
        {
          "alt": "贡献图表",
          "alt:en-US": "Github chart",
          "src": "https://ghchart.rshah.org/xiaohuohumax"
        }
      ]
    }
  ]
}