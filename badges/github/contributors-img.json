{
  "title": "Contributors Img",
  "description": "生成一个贡献者图片，让你的 README.md 文件保持同步。",
  "description:en-US": "Generate an image of contributors to keep your README.md in sync.",
  "tag": {
    "repo": "https://github.com/lacolaco/contributors-img",
    "onlineTool": "https://contrib.rocks/preview"
  },
  "linkRules": {
    "querys": [
      {
        "name": "repo",
        "type": "string",
        "description": "仓库名",
        "description:en-US": "Repository name",
        "moreDescription": "格式：`owner/repo`。",
        "moreDescription:en-US": "Format: `owner/repo`."
      },
      {
        "name": "max",
        "type": "number",
        "default": "100",
        "description": "最大显示贡献者数",
        "description:en-US": "Maximum number of contributors to display"
      },
      {
        "name": "columns",
        "type": "number",
        "default": "12",
        "description": "显示列数",
        "description:en-US": "Number of columns to display"
      },
      {
        "name": "anon",
        "type": "number",
        "default": "0",
        "description": "是否显示匿名贡献者",
        "description:en-US": "Whether to display anonymous contributors",
        "moreDescription": "可选值：`0` `1`。",
        "moreDescription:en-US": "Supported: `0` `1`."
      }
    ],
    "uriTemplate": "https://contrib.rocks/image{?repo,max,columns,anon}"
  },
  "examples": {
    "annotate": "lacolaco/contributors-img",
    "alt": "贡献",
    "alt:en-US": "Contributors Image",
    "href": "https://contrib.rocks/image?repo=lacolaco/contributors-img",
    "images": [
      {
        "alt": "贡献",
        "alt:en-US": "Contributors Image",
        "href": "https://contrib.rocks/image?repo=lacolaco/contributors-img"
      }
    ]
  }
}