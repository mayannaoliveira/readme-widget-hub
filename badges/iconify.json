{
  "title": "Iconify",
  "description": "Iconify API 可以动态生成 SVG，其中包含大量的图标集可供选择，可用于 HTML 或 CSS。",
  "description:en-US": "Iconify API can dynamically generate SVG, which includes a large number of icon sets for selection, and can be used for HTML or CSS.",
  "tags": {
    "repoUrl": "https://github.com/iconify/iconify",
    "officialDocsUrl": "https://iconify.design/docs/api/svg.html",
    "onlineToolUrl": "https://icon-sets.iconify.design/?category=Emoji"
  },
  "rules": {
    "params": [
      {
        "name": "prefix",
        "type": "string",
        "description": "图标集前缀",
        "description:en-US": "Icon set prefix"
      },
      {
        "name": "name",
        "type": "string",
        "description": "图标名称",
        "description:en-US": "Icon name"
      }
    ],
    "querys": [
      {
        "name": "color",
        "type": "string",
        "description": "图标颜色",
        "description:en-US": "Icon color",
        "extDescription": "支持格式：`HEX` `CSS 颜色值` 例如：`#FF0000` `red` `rgb(255, 0, 0)`",
        "extDescription:en-US": "Supported: `HEX` `CSS name` e.g. `#FF0000` `red` `rgb(255, 0, 0)`"
      },
      {
        "name": "width",
        "type": "number",
        "description": "图标宽度",
        "description:en-US": "Image width",
        "extDescription": "单位：像素",
        "extDescription:en-US": "Unit: pixels"
      },
      {
        "name": "height",
        "type": "number",
        "description": "图标高度",
        "description:en-US": "Image height",
        "extDescription": "单位：像素",
        "extDescription:en-US": "Unit: pixels"
      },
      {
        "name": "flip",
        "type": "string",
        "description": "翻转图标",
        "description:en-US": "Flip icon",
        "extDescription": "可选值：`horizontal` `vertical`",
        "extDescription:en-US": "Supported: `horizontal` `vertical`"
      },
      {
        "name": "rotate",
        "type": "string",
        "description": "旋转图标",
        "description:en-US": "Rotate icon",
        "extDescription": "格式：`deg` `number` 其中 `1=90deg`",
        "extDescription:en-US": "Format: `deg` `number` where `1=90deg`"
      },
      {
        "name": "download",
        "type": "boolean",
        "description": "强制浏览器下载文件",
        "description:en-US": "Forces browser to download file"
      },
      {
        "name": "box",
        "type": "boolean",
        "description": "是否向 SVG 添加与 viewBox 匹配的空矩形",
        "description:en-US": "Adds an empty rectangle to SVG that matches viewBox"
      }
    ],
    "srcTemplate": "https://api.iconify.design/{prefix}/{name}.svg{?color,width,height,flip,rotate,download,box}"
  },
  "examples": [
    {
      "explain": {
        "alt": "Iconify 1st Place Medal Badge",
        "src": "https://api.iconify.design/fluent-emoji-flat/1st-place-medal.svg?width=64"
      },
      "images": [
        {
          "alt": "Iconify 1st Place Medal Badge",
          "src": "https://api.iconify.design/fluent-emoji-flat/1st-place-medal.svg?width=64"
        }
      ]
    }
  ]
}