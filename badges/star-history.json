{
  "title": "Star History | GitHub 星标历史图",
  "description": "Star History 一个可以显示 GitHub 项目的 star 历史图的徽章。",
  "tags": {
    "repoUrl": "https://github.com/star-history/star-history",
    "onlineToolUrl": "https://www.star-history.com",
    "officialDocsUrl": "https://www.star-history.com/blog/how-to-use-github-star-history"
  },
  "rules": {
    "querys": [
      {
        "name": "repos",
        "type": "string[]",
        "description": "仓库名",
        "extDescription": "多个仓库名用逗号分隔，例如：`owner1/repo1,owner2/repo2`"
      },
      {
        "name": "type",
        "type": "string",
        "description": "显示类型",
        "extDescription": "可选值：`Date` `Timeline`"
      }
    ],
    "srcTemplate": "https://api.star-history.com/svg{?repos,type}"
  },
  "examples": [
    {
      "explain": {
        "annotate": "star-history/star-history",
        "alt": "Star History Chart",
        "src": "https://api.star-history.com/svg?repos=star-history/star-history&type=Date"
      },
      "images": [
        {
          "alt": "Star History Chart",
          "src": "https://api.star-history.com/svg?repos=star-history/star-history&type=Date"
        }
      ]
    }
  ]
}