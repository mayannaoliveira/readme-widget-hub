{
  "title": "Open Collective",
  "description": "创建一个 SVG，展示您的财务贡献者的卡片。",
  "description:en-US": "Create an SVG showing the icons of your financial contributors.",
  "tags": {
    "repoUrl": "https://github.com/opencollective",
    "officialDocsUrl": "https://docs.opencollective.com/help/collectives/collective-settings/data-export#contributor-image"
  },
  "rules": {
    "params": [
      {
        "name": "collectiveName",
        "type": "string",
        "description": "Open Collective 组织名",
        "description:en-US": "Open Collective organization name"
      },
      {
        "name": "statisticalType",
        "type": "string",
        "description": "统计类型",
        "description:en-US": "Statistics type",
        "extDescription": "可选值：`sponsors`（赞助商）`backers`（支持者）`contributors`（贡献者）。",
        "extDescription:en-US": "supported: `sponsors` `backers` `contributors`."
      }
    ],
    "querys": [
      {
        "name": "width",
        "type": "number",
        "description": "图片宽度",
        "description:en-US": "Width of the image",
        "extDescription": "单位：像素。",
        "extDescription:en-US": "Unit: pixels."
      },
      {
        "name": "height",
        "type": "number",
        "description": "图片高度",
        "description:en-US": "Height of the image",
        "extDescription": "单位：像素。",
        "extDescription:en-US": "Unit: pixels."
      },
      {
        "name": "limit",
        "type": "number",
        "description": "最多显示成员数",
        "description:en-US": "Max number of members to show",
        "extDescription": "空则表示：无上限。",
        "extDescription:en-US": "Empty means: unlimited."
      },
      {
        "name": "avatarHeight",
        "type": "number",
        "description": "每个头像/标志的最大高度",
        "description:en-US": "Max height of each avatar / logo"
      },
      {
        "name": "button",
        "type": "boolean",
        "default": "true",
        "description": "显示 “成为支持者/赞助商” 按钮",
        "description:en-US": "Show 'become a backer/sponsor' button"
      },
      {
        "name": "format",
        "type": "string",
        "description": "图片格式",
        "description:en-US": "Format of the image",
        "extDescription": "将 `.svg` 替换为 `.png` 或 `.jpg`。",
        "extDescription:en-US": "Replace `.svg` with `.png` or `.jpg`."
      }
    ],
    "srcTemplate": "https://opencollective.com/{collectiveName}/{statisticalType}.svg{?width,height,limit,avatarHeight,button,format}"
  },
  "examples": [
    {
      "explain": {
        "annotate": "vitejs/vite",
        "alt": "Sponsors on Open Collective",
        "src": "https://opencollective.com/vite/sponsors.svg"
      },
      "images": [
        {
          "alt": "Sponsors on Open Collective",
          "src": "https://opencollective.com/vite/sponsors.svg"
        }
      ]
    },
    {
      "explain": {
        "annotate": "vitejs/vite",
        "alt": "Sponsors Badge on Open Collective",
        "src": "https://opencollective.com/vite/sponsors/badge.svg"
      },
      "images": [
        {
          "alt": "Sponsors Badge on Open Collective",
          "src": "https://opencollective.com/vite/sponsors/badge.svg"
        }
      ]
    }
  ]
}