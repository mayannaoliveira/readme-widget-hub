{
  "cSpell.words": [
    "esbuild",
    "navs",
    "npmjs",
    "querys",
    "tocs",
    "vite"
  ],
  "cSpell.ignorePaths": [
    "package.json",
    "package-lock.json",
    "node_modules",
    ".git/{info,lfs,logs,refs,objects}/**",
    ".git/{index,*refs,*HEAD}",
    ".vscode",
    ".github",
    "badges",
    "README*.md"
  ],
  "json.schemas": [
    {
      "fileMatch": [
        "/badges/**/*.json",
        "!/badges/**/collection.json"
      ],
      "url": "/packages/badge/schema/badge.schema.json"
    },
    {
      "fileMatch": [
        "/badges/**/collection.json"
      ],
      "url": "/packages/badge/schema/collection.schema.json"
    },
    {
      "fileMatch": [
        "/meta.json"
      ],
      "url": "/packages/meta/schema/meta.schema.json"
    }
  ]
}