{
  "cSpell.ignorePaths": [
    ".git",
    ".vscode",
    ".github",
    "node_modules",
    "dist",
    "/*.md",
    "*.json",
    ".env*",
    "eslint.config.mjs",
    "commitlint.config.cjs"
  ],
  "cSpell.words": [
    "Jsons",
    "klaw",
    "nodir"
  ],
  "json.schemas": [
    {
      "fileMatch": [
        "/badge/**/*.json"
      ],
      "url": "/.vscode/schema/badge.schema.json"
    },
    {
      "fileMatch": [
        "/scripts/spider/config.json"
      ],
      "url": "/.vscode/schema/spider.schema.json"
    }
  ]
}