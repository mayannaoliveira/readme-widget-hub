{
  "cSpell.words": [
    "esbuild",
    "navs",
    "npmjs",
    "querys",
    "tocs",
    "vite"
  ],
  "cSpell.ignorePaths": [
    "package.json",
    "pnpm-lock.yaml",
    "node_modules",
    ".git/{info,lfs,logs,refs,objects}/**",
    ".git/{index,*refs,*HEAD}",
    ".vscode",
    ".github",
    "widgets",
    "README*.md"
  ],
  "json.schemas": [
    {
      "fileMatch": [
        "/widgets/**/*.json",
        "!/widgets/**/collection.json"
      ],
      "url": "/packages/widget/schema/widget.schema.json"
    },
    {
      "fileMatch": [
        "/widgets/**/collection.json"
      ],
      "url": "/packages/widget/schema/collection.schema.json"
    },
    {
      "fileMatch": [
        "/meta.json"
      ],
      "url": "/packages/meta/schema/meta.schema.json"
    }
  ]
}