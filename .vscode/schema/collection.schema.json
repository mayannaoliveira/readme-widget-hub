{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Collection",
  "definitions": {
    "Collection": {
      "type": "object",
      "properties": {
        "enabled": {
          "type": "boolean",
          "description": "是否启用",
          "default": true
        },
        "index": {
          "type": "number",
          "description": "显示顺序，数字越小越靠前",
          "default": 9999
        },
        "title": {
          "type": "string",
          "description": "名称"
        },
        "description": {
          "type": "string",
          "description": "描述"
        }
      },
      "required": [
        "title"
      ],
      "additionalProperties": false
    }
  }
}
